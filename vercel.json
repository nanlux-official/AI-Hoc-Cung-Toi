{
  "buildCommand": "cd client && npm install && npm run build",
  "outputDirectory": "client/build",
  "framework": null,
  "functions": {
    "api/gemini.js": {
      "runtime": "nodejs18.x"
    }
  },
  "rewrites": [
    {
      "source": "/api/gemini/generate",
      "destination": "/api/gemini.js"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
